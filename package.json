  "devDependencies": {
    "cssnano": "^4.1.10",
    "eslint-plugin-react-hooks": "^1.6.0",
    "flow-bin": "^0.98.0",
    "fs-extra": "^7.0.1",
    "node-sass": "^4.12.0",
    "postcss": "^7.0.16",
    "postcss-cli": "^6.1.2",
    "prettier": "1.17.0",
    "react-app-rewired": "^2.1.3",
    "websocket": "^1.0.28",
    "worker-loader": "^2.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "build": "npm run sass:build && react-app-rewired build",
    "flow:watch": "npm run flow:watch:fswatch",
    "flow:watch:fswatch": "fswatch --one-event --one-per-batch src | xargs -n1 -I{} yarn run flow status",
    "flow:watch:inotify": "inotifywait -e close_write -r -m src | xargs -n1 -I{} yarn run flow status",
    "postbuild": "npm run sass:postbuild",
    "prettier": "./node_modules/.bin/prettier --write \"./{flow-typed,public,scss,src}/**/*.{htm,html,js,scss}\"",
    "sass:build": "./node_modules/.bin/node-sass -r ./scss -o ./public",
    "sass:postbuild": "postcss < public/index.css > public/index.css.tmp && mv public/index.css.tmp public/index.css",
    "sass:watch": "./node_modules/.bin/node-sass -r -w ./scss -o ./public",
    "start": "PORT=9023 react-app-rewired start",
    "test": "react-app-rewired test --env=jsdom"
  }
}
