  "dependencies": {
    "@tweenjs/tween.js": "^17.4.0",
    "auto-bind": "^2.1.0",
    "clamp": "^1.0.1",
    "classnames": "^2.2.6",
    "core-js": "^3.1.4",
    "debounce": "^1.2.0",
    "eventemitter3": "^4.0.0",
    "fengari": "^0.1.4",
    "fengari-interop": "^0.1.2",
    "howler": "^2.1.2",
    "immutable": "^4.0.0-rc.12",
    "javascript-state-machine": "^3.1.0",
    "jsdom": "^15.1.1",
    "jsonrpc-lite": "^2.0.7",
    "lodash": "^4.17.14",
    "mainloop.js": "^1.0.4",
    "pathfinding": "^0.4.18",
    "raf": "^3.4.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-draggable": "^3.3.0",
    "react-markdown": "^4.1.0",
    "react-outside-click-handler": "^1.2.3",
    "react-relay": "^5.0.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.0.1",
    "resize-observer-polyfill": "^1.5.1",
    "three": "^0.106.2",
    "twig": "^1.13.3",
    "xmldom": "^0.1.27",
    "yaml": "^1.6.0",
    "yn": "^3.1.0"
  },
  "devDependencies": {
    "cssnano": "^4.1.10",
    "eslint-plugin-react-hooks": "^1.6.0",
    "flow-bin": "^0.102.0",
    "fs-extra": "^8.1.0",
    "node-sass": "^4.12.0",
    "postcss": "^7.0.17",
    "postcss-cli": "^6.1.3",
    "prettier": "1.18.2",
    "websocket": "^1.0.29"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "build": "react-scripts build",
    "flow:watch": "$npm_execpath run flow:watch:fswatch",
    "flow:watch:fswatch": "fswatch --one-event --one-per-batch src | xargs -n1 -I{} yarn run flow status",
    "flow:watch:inotify": "inotifywait -e close_write -r -m src | xargs -n1 -I{} yarn run flow status",
    "postbuild": "$npm_execpath run sass:postbuild",
    "prettier": "./node_modules/.bin/prettier --write \"./{flow-typed,public,scss,src}/**/*.{htm,html,js,scss}\"",
    "sass:build": "./node_modules/.bin/node-sass -r ./scss -o ./build",
    "sass:postbuild": "postcss < build/index.css > build/index.css.tmp && mv build/index.css.tmp build/index.css",
    "sass:watch": "./node_modules/.bin/node-sass -r -w ./scss -o ./public",
    "start": "PORT=9023 react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "test:once": "$npm_execpath run test --watchAll=false"
  }
}
