    "partykals": "^1.0.1",
    "raf": "^3.4.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-markdown": "^4.3.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.1",
    "resize-observer-polyfill": "^1.5.1",
    "screenfull": "^5.0.1",
    "three": "^0.113.2",
    "yaml": "^1.7.2",
    "yn": "^4.0.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^25.1.1",
    "@types/lodash": "^4.14.149",
    "@types/mainloop.js": "^1.0.5",
    "@types/mathjs": "^6.0.4",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/yaml": "^1.2.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "fs-extra": "^8.1.0",
    "node-sass": "^4.13.1",
    "prettier": "1.19.1",
    "prettier-plugin-import-sort": "^0.0.3",
    "typescript": "^3.7.5",
    "websocket": "^1.0.31"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "importSort": {
    ".js, .ts, .tsx": {
      "style": "personalidol"
    }
  },
  "jest": {
    "coverageReporters": [
      "html"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!three).+\\.js$"
    ]
  },
  "scripts": {
    "build": "react-scripts build",
    "modernizr": "modernizr -c ./modernizr.json -d ./public/vendor -u",
    "prettier": "prettier --write \"./{public,src}/**/*.{htm,html,js,scss,ts,tsx}\"",
    "start": "PORT=9023 PUBLIC_URL=$REACT_APP_PUBLIC_URL react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "test:once": "$npm_execpath run test --watchAll=false",
    "test:once:coverage": "$npm_execpath run test:once --coverage",
    "typescript:watch": "tsc --watch --noEmit"
  }
}
