  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "build": "npm run sass:build && react-scripts build",
    "flow:watch": "fswatch src | xargs -n1 -I{} yarn run flow status",
    "postbuild": "npm run sass:postbuild",
    "prettier": "./node_modules/.bin/prettier --write \"./{public,scss,src}/**/*.{htm,html,js,scss}\"",
    "sass:build": "./node_modules/.bin/node-sass -r ./scss -o ./public",
    "sass:postbuild": "postcss < public/index.css > public/index.css.tmp && mv public/index.css.tmp public/index.css",
    "sass:watch": "./node_modules/.bin/node-sass -r -w ./scss -o ./public",
    "start": "PORT=3100 react-scripts start",
    "test": "react-scripts test --env=jsdom"
  }
