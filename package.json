    "eventemitter3": "^4.0.0",
    "fengari": "^0.1.4",
    "fengari-interop": "^0.1.2",
    "howler": "^2.1.2",
    "immutable": "^4.0.0-rc.12",
    "javascript-state-machine": "^3.1.0",
    "jsdom": "^15.2.0",
    "jsonrpc-lite": "^2.1.0",
    "lodash": "^4.17.15",
    "mainloop.js": "^1.0.4",
    "modernizr": "^3.8.0",
    "ola": "^1.1.0",
    "pathfinding": "^0.4.18",
    "raf": "^3.4.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-markdown": "^4.2.2",
    "react-outside-click-handler": "^1.3.0",
    "react-relay": "^7.0.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.2.0",
    "three": "^0.109.0",
    "twig": "^1.13.3",
    "xmldom": "^0.1.27",
    "yaml": "^1.7.2",
    "yn": "^3.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.0",
    "cssnano": "^4.1.10",
    "eslint-plugin-react-hooks": "^2.2.0",
    "flow-bin": "^0.110.1",
    "fs-extra": "^8.1.0",
    "node-sass": "^4.13.0",
    "postcss": "^7.0.21",
    "postcss-cli": "^6.1.3",
    "prettier": "1.18.2",
    "websocket": "^1.0.30"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "build": "react-scripts build",
    "flow:watch": "$npm_execpath run flow:watch:fswatch",
    "flow:watch:fswatch": "fswatch --one-event --one-per-batch src | xargs -n1 -I{} yarn run flow status",
    "flow:watch:inotify": "inotifywait -e close_write -r -m src | xargs -n1 -I{} yarn run flow status",
    "modernizr": "./node_modules/.bin/modernizr -c ./node_modules/modernizr/lib/config-all.json -d ./public/vendor -u",
    "postinstall": "$npm_execpath run modernizr",
    "prettier": "./node_modules/.bin/prettier --write \"./{flow-typed,public,scss,src}/**/*.{htm,html,js,scss}\"",
    "sass:build": "./node_modules/.bin/node-sass -r ./scss -o ./build && $npm_execpath run sass:postcss",
    "sass:postcss": "postcss -r build/index.css",
    "sass:watch": "./node_modules/.bin/node-sass -r -w ./scss -o ./public",
    "start": "PORT=9023 react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "test:once": "$npm_execpath run test --watchAll=false"
  }
}
